#include<stdio.h>

int main(void)
{
	char str[] = " show me the money";

	int i = 0;

	while (str[i] != 0)
		i++;

	printf("문자열 %s의 길이는 %d입니다.\n", str, i);

	return 0;
}


#include<stdio.h>
#define _CRT_SECURE_NO_WARNINGS

int main(void)
{
	char line[100] = "";
	char line2[100] = "";

	printf("문자열을 입력하시오:");
	ge_s(line, sizeof(line));// 한 줄의 문자열 입력

	int k = 0;
	for (int i = 0; line[i] != '\0'; ++i)
	{
		while ((line[i] >= 'A' && line[i] <= 'Z') || (line[i] >= 'a' && line[i] <= 'z'))
		{
		line2[K++] = line[i++]; 
		}

	}
	line2[K] = '\0';
	printf("출력 문자열: %s\n", line2);
	return 0;
}
